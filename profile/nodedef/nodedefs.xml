<nodeDefs> 
   <nodeDef id="controller" nls="nls_ctrl">
      <editors />
      <sts>
         <st id="ST" editor="Enabled" />
         <st id="GV0" editor="IrrState" />
         <st id="GV1" editor="Nbr_Zones" />
         <st id="GV10" editor="Sch_Source" />
          <st id="GV11" editor="Sch_Status" />
         <st id="GV18" editor="LastTime" />
